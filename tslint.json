{
  "extends": ["tslint:recommended", "tslint-react", "tslint-config-prettier"],
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts",
      "coverage/lcov-report/*.js"
    ]
  },
  "rules": {
    "jsx-no-lambda": false,
    "interface-over-type-literal": false,
    "curly": false,
    "no-console": false,
    "variable-name": [true, "ban-keywords", "check-format", "allow-leading-underscore"]
  }
}
